{
  "type": "module",
  "name": "@graphyn/code",
  "version": "0.1.71",
  "description": "AI-powered CLI that orchestrates context-aware development agents for Claude Code",
  "main": "dist/cli/main.js",
  "bin": {
    "graphyn": "bin/graphyn.js"
  },
  "scripts": {
    "build": "tsc --build",
    "build:packages": "tsc --build packages/*/tsconfig.json",
    "build:apps": "tsc --build apps/*/tsconfig.json",
    "build:services": "tsc --build services/*/tsconfig.json",
    "build:clean": "tsc --build --clean",
    "dev": "tsx apps/cli/src/index.ts",
    "dev:cli": "tsx apps/cli/src/index.ts",
    "dev:mcp": "tsx services/mcp/src/index.ts --mock",
    "dev:coordinator": "tsx services/coordinator/src/index.ts",
    "dev:dashboard": "cd apps/mission-control && pnpm dev",
    "start": "node apps/cli/dist/index.js",
    "start:mcp": "node services/mcp/dist/index.js",
    "start:coordinator": "node services/coordinator/dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:package": "./scripts/test-package.sh",
    "lint": "pnpm --filter '@graphyn/*' run lint",
    "lint:fix": "pnpm --filter '@graphyn/*' run lint:fix",
    "clean": "pnpm --recursive run clean",
    "validate": "node scripts/validate-package.cjs",
    "prepublishOnly": "pnpm run build && pnpm run test:package",
    "postinstall": "node scripts/postinstall.js || true",
    "flow:generate": "tsx tools/flowgen.ts",
    "flow:validate": "vitest run tests/flow/validate.spec.ts",
    "flow:render": "tsx tools/flowrender.ts"
  },
  "keywords": [
    "ai",
    "development",
    "cli",
    "claude-code",
    "graphyn",
    "assistant",
    "backend",
    "frontend",
    "architect"
  ],
  "author": "Graphyn",
  "license": "MIT",
  "homepage": "https://graphyn.xyz/code",
  "repository": {
    "type": "git",
    "url": "https://github.com/fuego-wtf/graphyn-code.git"
  },
  "bugs": {
    "url": "https://github.com/fuego-wtf/graphyn-code/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.108",
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@types/blessed": "^0.1.25",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "@types/which": "^3.0.4",
    "@types/yargs": "^17.0.33",
    "axios": "^1.6.0",
    "better-sqlite3": "^12.2.0",
    "blessed": "^0.1.81",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "clipboardy": "^4.0.0",
    "command-exists": "^1.2.9",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1",
    "eventsource": "^2.0.2",
    "fast-glob": "^3.3.2",
    "figlet": "^1.6.0",
    "glob": "^10.4.5",
    "gradient-string": "^2.0.2",
    "gray-matter": "^4.0.3",
    "inquirer": "^12.9.4",
    "lru-cache": "^11.1.0",
    "meow": "^13.0.0",
    "node-fetch": "^3.3.2",
    "open": "^10.0.0",
    "ora": "^8.2.0",
    "simple-git": "^3.20.0",
    "sqlite": "^4.2.1",
    "uuid": "^11.1.0",
    "which": "^5.0.0",
    "yargs": "^18.0.0",
    "zod": "^4.1.5",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/eventsource": "^1.1.15",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "eslint": "^8.57.1",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "yaml": "^2.8.1"
  },
  "files": [
    "dist",
    "bin",
    "prompts",
    "templates",
    "scripts/postinstall.js",
    "README.md",
    "LICENSE"
  ]
}
