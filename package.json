{
  "name": "@graphyn/code",
  "version": "0.1.61",
  "description": "AI-powered CLI that orchestrates context-aware development agents for Claude Code",
  "type": "module",
  "main": "dist/ink/cli.js",
  "bin": {
    "graphyn": "./dist/graphyn-wrapper.js"
  },
  "scripts": {
    "build": "./scripts/build-ink.sh",
    "build:ink": "tsc --module ESNext --target ES2022 --jsx react --esModuleInterop --skipLibCheck --moduleResolution bundler --outDir dist",
    "dev": "tsx src/ink/cli.tsx",
    "dev:minimal": "tsx src/minimal-app.tsx",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:package": "./scripts/test-package.sh",
    "test:squad": "node scripts/run-squad-test-info.js",
    "test:squad:mock": "node scripts/run-squad-test-info.js",
    "validate": "node scripts/validate-package.cjs",
    "prepublishOnly": "npm run build && npm run test:package",
    "postinstall": "node scripts/postinstall.js || true"
  },
  "keywords": [
    "ai",
    "development",
    "cli",
    "claude-code",
    "graphyn",
    "assistant",
    "backend",
    "frontend",
    "architect"
  ],
  "author": "Graphyn",
  "license": "MIT",
  "homepage": "https://graphyn.xyz/code",
  "repository": {
    "type": "git",
    "url": "https://github.com/fuego-wtf/graphyn-code.git"
  },
  "bugs": {
    "url": "https://github.com/fuego-wtf/graphyn-code/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.33",
    "axios": "^1.6.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "clipboardy": "^4.0.0",
    "command-exists": "^1.2.9",
    "dotenv": "^16.3.1",
    "eventsource": "^2.0.2",
    "fast-glob": "^3.3.2",
    "figlet": "^1.6.0",
    "glob": "^10.4.5",
    "gradient-string": "^2.0.2",
    "gray-matter": "^4.0.3",
    "ink": "^5.0.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^9.2.12",
    "lru-cache": "^11.1.0",
    "meow": "^13.0.0",
    "node-fetch": "^3.3.2",
    "open": "^10.0.0",
    "ora": "^8.2.0",
    "react": "^18.2.0",
    "simple-git": "^3.20.0",
    "uuid": "^11.1.0",
    "yargs": "^18.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.22.0",
    "@types/eventsource": "^1.1.15",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^1.6.1",
    "ink-testing-library": "^3.0.0",
    "openapi-typescript": "^7.9.1",
    "ts-node": "^10.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2",
    "vitest": "^1.6.1"
  },
  "files": [
    "dist",
    "prompts",
    "templates",
    "scripts/postinstall.js",
    "README.md",
    "LICENSE"
  ]
}
