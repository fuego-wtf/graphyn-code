{
  "name": "@graphyn/code",
  "version": "0.1.70",
  "description": "AI-powered CLI that orchestrates context-aware development agents for Claude Code",
  "type": "module",
  "main": "dist/cli-orchestrator.js",
  "bin": {
    "graphyn": "bin/graphyn.js"
  },
  "scripts": {
    "build": "tsc --module ESNext --target ES2022 --esModuleInterop --skipLibCheck --moduleResolution bundler --outDir dist",
    "dev": "tsx src/cli.ts",
    "dev:old": "tsx src/cli-orchestrator.ts",
    "start": "node dist/cli.js",
    "dev:minimal": "tsx src/minimal-app.tsx",
    "demo:enhanced-ux": "tsx src/examples/enhanced-ux-demo.ts",
    "figma": "tsx src/cli.ts figma",
    "cli": "tsx src/cli.ts",
    "figma:setup": "tsx src/cli.ts figma setup",
    "figma:status": "tsx src/cli.ts figma status",
    "figma:agents": "tsx src/cli.ts figma agents",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:package": "./scripts/test-package.sh",
    "test:squad": "node scripts/run-squad-test-info.js",
    "test:squad:mock": "node scripts/run-squad-test-info.js",
    "validate": "node scripts/validate-package.cjs",
    "prepublishOnly": "npm run build && npm run test:package",
    "postinstall": "node scripts/postinstall.js || true"
  },
  "keywords": [
    "ai",
    "development",
    "cli",
    "claude-code",
    "graphyn",
    "assistant",
    "backend",
    "frontend",
    "architect"
  ],
  "author": "Graphyn",
  "license": "MIT",
  "homepage": "https://graphyn.xyz/code",
  "repository": {
    "type": "git",
    "url": "https://github.com/fuego-wtf/graphyn-code.git"
  },
  "bugs": {
    "url": "https://github.com/fuego-wtf/graphyn-code/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.108",
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "@types/which": "^3.0.4",
    "@types/yargs": "^17.0.33",
    "axios": "^1.6.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "clipboardy": "^4.0.0",
    "command-exists": "^1.2.9",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1",
    "eventsource": "^2.0.2",
    "fast-glob": "^3.3.2",
    "figlet": "^1.6.0",
    "glob": "^10.4.5",
    "gradient-string": "^2.0.2",
    "gray-matter": "^4.0.3",
    "inquirer": "^12.9.4",
    "lru-cache": "^11.1.0",
    "meow": "^13.0.0",
    "node-fetch": "^3.3.2",
    "open": "^10.0.0",
    "ora": "^8.2.0",
    "simple-git": "^3.20.0",
    "uuid": "^11.1.0",
    "which": "^5.0.0",
    "yargs": "^18.0.0",
    "zod": "^4.1.5",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/eventsource": "^1.1.15",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "openapi-typescript": "^7.9.1",
    "ts-node": "^10.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "bin",
    "prompts",
    "templates",
    "scripts/postinstall.js",
    "README.md",
    "LICENSE"
  ]
}
