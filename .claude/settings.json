      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/resatugurulu/Developer/graphyn-workspace/code"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
